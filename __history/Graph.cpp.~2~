#pragma package(smart_init)
#pragma resource "*.dfm"
TForm1 *Form1;
//---------------------------------------------------------------------------
__fastcall TForm1::TForm1(TComponent* Owner)
	: TForm(Owner)
{
}
//---------------------------------------------------------------------------
void __fastcall TForm1::FormActivate(TObject *Sender)
{
	 Edit1->Text = "30";
     Edit2->Text = "-5";
     Edit3->Text = "5";
	 Edit4->Text = "-2";
     Edit5->Text = "2";
	 Edit1->SetFocus(); // установить фокус ввода в Edit1
}
